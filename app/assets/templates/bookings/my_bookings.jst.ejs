<section class="my-listings-toggler">
	<ul>
		<li><a href="#/my_listings">My Listings</a></li>
		<li>My Booking Requests</li>
	</ul>
</section>

<section class="my-listings-list group">
	<% if (bookings.length > 0) { %>
		<ul>
			<% bookings.each(function(booking) { %>
				<li><%= booking.get("user").fname %> | Dates requested:	<%= booking.escape("start_date") %> - <%= booking.escape("end_date") %></li>
				<% if (booking.escape("status") === "pending") { %>
					<li class="approve-deny-buttons">
						<button class="approve" data-id="<%= booking.id %>">Approve</button>
						<button class="deny" data-id="<%= booking.id %>">Deny</button>
					</li>
				<% } else if (booking.escape("status") === "approved") { %>
					<li>Approved</li>
				<% } else if (booking.escape("status") === "denied") { %>
					<li>Denied</li>
				<% } %>
			<% }); %>
		</ul>
	<% } else { %>
		<p>Your listings have no outstanding requests.</p>
	<% } %>
</section>